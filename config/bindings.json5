  {
    "agentId": "ops-monitor",
    "match": {
      "channel": "discord",
      "guildId": "${DISCORD_SERVER_ID_OPS}"
    }
  },
  {
    "agentId": "ops-monitor",
    "match": {
      "channel": "discord",
      "guildId": "${DISCORD_SERVER_ID_DEV}"
    }
  },
  {
    "agentId": "demo-controller",
    "match": {
      "channel": "discord",
      "guildId": "${DISCORD_SERVER_ID_OPS}"
    }
  },
  {
    "agentId": "demo-controller",
    "match": {
      "channel": "discord",
      "guildId": "${DISCORD_SERVER_ID_DEV}"
    }
  },
  {
    "agentId": "jira-resolver",
    "match": {
      "channel": "discord",
      "guildId": "${DISCORD_SERVER_ID_OPS}"
    }
  },
  {
    "agentId": "jira-resolver",
    "match": {
      "channel": "discord",
      "guildId": "${DISCORD_SERVER_ID_DEV}"
    }
  }
]
