{
  "meta": {
    "lastTouchedVersion": "2026.2.9",
    "lastTouchedAt": "2026-02-10T05:37:54.704Z"
  },
  "env": {
    "shellEnv": { "enabled": true }
  },
  "wizard": {
    "lastRunAt": "2026-02-10T05:13:48.093Z",
    "lastRunVersion": "2026.2.9",
    "lastRunCommand": "doctor",
    "lastRunMode": "local"
  },
  "logging": {
    "redactSensitive": "tools"
  },
  "browser": {
    "enabled": true,
    "defaultProfile": "chrome",
    "profiles": {
      "chrome": {
        "cdpPort": 18792,
        "color": "4285F4"
      },
      "playwright": {
        "cdpPort": 9223,
        "color": "2ECC71"
      }
    }
  },
  "auth": {
    "profiles": {
      "google-antigravity:your-email@example.com": {
        "provider": "google-antigravity",
        "mode": "oauth",
        "email": "${JIRA_EMAIL}"
      },
      "anthropic:default": {
        "provider": "anthropic",
        "mode": "token"
      },
      "openai-codex:default": {
        "provider": "openai-codex",
        "mode": "oauth"
      }
    }
  },
  "agents": { "$include": "./config/agents.json5" },
  "tools": {
    "web": {
      "search": {
        "enabled": true,
        "apiKey": "${SERPER_API_KEY}"
      },
      "fetch": {
        "enabled": true
      }
    }
  },
  "bindings": { "$include": "./config/bindings.json5" },
  "messages": {
    "queue": {
      "mode": "collect",
      "cap": 20
    },
    "inbound": {
      "debounceMs": 2000
    },
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "session": {
    "dmScope": "per-channel-peer",
    "reset": {
      "mode": "daily",
      "atHour": 4
    }
  },
  "hooks": {
    "internal": {
      "enabled": true,
      "entries": {
        "boot-md": { "enabled": true },
        "command-logger": { "enabled": true },
        "session-memory": { "enabled": true }
      }
    }
  },
  "channels": { "$include": "./config/channels.json5" },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "${GATEWAY_AUTH_TOKEN}"
    },
    "tailscale": {
      "mode": "off",
      "resetOnExit": false
    }
  },
  "skills": {
    "install": {
      "nodeManager": "bun"
    },
    "entries": {
      "nano-banana-pro": { "apiKey": "no" },
      "notion": { "apiKey": "no" }
    }
  },
  "plugins": {
    "entries": {
      "google-antigravity-auth": { "enabled": true },
      "imessage": { "enabled": true },
      "discord": { "enabled": true }
    }
  }
}
